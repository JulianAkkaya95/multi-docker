{
  "AWSEBDockerrunVersion": 2,
  "contaierDefinitions": [
    {
      "name": "client",
      "image": "julianakk/multi-client",
      "hostname": "client",
      "essentail": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "julianakk/multi-server",
      "hostname": "api",
      "essentail": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "julianakk/multi-worker",
      "hostname": "worker",
      "essentail": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "julianakk/multi-nginx",
      "essentail": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}